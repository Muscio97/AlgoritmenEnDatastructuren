from inspect import stack

class Mystack:
    global stack
    stack = []
    
    def push(item):
        stack.extend([item])
        
    def pop():
        if(len(stack) > 0):
            temp = stack[-1]
            del stack[-1]
            return temp
        return 0
    
    def peek():
        if(len(stack) > 0):
            return stack[-1]   
        return 0
    
    def isEmpty():
        return len(stack)

        
Mystack.push('<')
print(Mystack.peek())


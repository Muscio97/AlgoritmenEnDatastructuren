from Week_2_2 import Mystack

class Checker(string):
    

    def equal_check(item):
        if(item ==')'):
            if(Mystack.peek()=='('):
                return True
            return False
        if(item ==']'):
            if(Mystack.peek()=='['):
                return True
            return False           
        if(item =='}'):
            if(Mystack.peek()=='{'):
                return True
            return False
        
    for element in string:
        if(element =='(' or element =='{' or element =='['):
            Mystack.push(element)
        if(element ==')' or element =='}' or element ==']'):
            if(equal_check(element)==True):
                Mystack.pop()
            else
                return False
    
    return True




Checker(input("Str: "))
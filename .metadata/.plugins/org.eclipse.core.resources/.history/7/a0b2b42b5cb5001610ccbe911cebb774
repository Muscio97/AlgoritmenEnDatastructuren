from Week_2_2 import Mystack

class Checker():
    string = input("String: ")
   

    def equal_check():
         falid = 1
        for element in string:
            if(element =='(' or element =='{' or element =='['):
                Mystack.push(element)
            if(element ==')' or element =='}' or element ==']'):
                if(element ==')'):
                    if(Mystack.peek()=='('):
                        Mystack.pop()
                    else:
                        return falid
                if(element ==']'):
                    if(Mystack.peek()=='['):
                        Mystack.pop()
                    else:
                        return falid          
                if(element =='}'):
                    if(Mystack.peek()=='{'):
                        Mystack.pop()
                    else:
                        return falid

Checker()
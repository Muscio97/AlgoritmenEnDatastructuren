def machtv3(a,n):
    assert n > 0
    m = 1
    while n > 0:
        
        if n%2 == 0:
            m = m * (a**2)
            n = n // 2
        else:
            m = m * a**(n-1)
            n = n - 1
    return int(m)


print(machtv3(2, 15))
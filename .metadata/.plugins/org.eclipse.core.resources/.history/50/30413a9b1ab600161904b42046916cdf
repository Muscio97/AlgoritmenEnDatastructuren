temp_str = input("Uw string alstublieft: ")

def mybin(bin_string):
    temp = int(bin_string)
    assert temp >=0
    
    if(temp % 2 == 0):
        print(2)
        temp = temp // 2
    else:
        print(1)
        temp =-1

    return mybin(temp)
        
        
print(mybin(temp_str))
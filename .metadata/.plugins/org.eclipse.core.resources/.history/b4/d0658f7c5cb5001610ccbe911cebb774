from Week_2_2 import Mystack

class Checker():

    def equal_check(temp):
        falid = 1
        for element in temp:
            if(element =='(' or element =='{' or element =='['):
                Mystack.push(element)
            if(element ==')' or element =='}' or element ==']'):
                if(element ==')'):
                    if(Mystack.peek()=='('):
                        Mystack.pop()
                    else:
                        return falid
                if(element ==']'):
                    if(Mystack.peek()=='['):
                        Mystack.pop()
                    else:
                        return falid          
                if(element =='}'):
                    if(Mystack.peek()=='{'):
                        Mystack.pop()
                    else:
                        return falid

Checker.equal_check(input("String: "))
from inspect import stack

class Mystack():
    global stack
    stack = []
    
    def push(item):
        stack.extend([item])
        
    def pop():
        if(len(stack) > 0):
            temp = stack[-1]
            del stack[-1]
            return temp
        return 0
    
    def peek():
        if(len(stack) > 0):
            return stack[-1]   
        return 0
    
    def isEmpty():
        print(stack)
        for i in int(len(stack)):
            del stack[i]
        print(stack)
        
Mystack.push(1)   
print(Mystack.peek())
Mystack.push(2)
print(Mystack.peek())
Mystack.push(3)
print(Mystack.peek())
Mystack.pop()
print(Mystack.peek())
Mystack.isEmpty()
print(Mystack.peek())
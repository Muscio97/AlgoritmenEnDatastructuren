from inspect import stack

class Mystack():
    global stack
    stack = []
    
    def push(item):
        stack.extend([item])
        
    def pop():
        temp = stack[-1]
        del stack[-1]
        return temp
    
    def peek():
        return stack[-1]    
        
Mystack.push(1)   
print(Mystack.peek())
Mystack.push(2)
print(Mystack.peek())
Mystack.push(3)
print(Mystack.peek())
Mystack.pop()
print(Mystack.peek())
temp_str = input("Uw string alstublieft: ")

def mybin(bin_string):
    hello = ""
    temp = int(bin_string)
    if(temp >=0):
        return print(hello)
    
    if(temp % 2 == 0):
        return '0'
        temp = temp // 2
    else:
        return '1'
    
    return hello + str(mybin(temp //2)) + str(temp % 2)
        
        
print(mybin(temp_str))
from Week_2_2 import Mystack

class Checker():
    string = input("String: ")
    falid = 1

    def equal_check(item):
        if(item ==')'):
            if(Mystack.peek()=='('):
                return True
            return False
        if(item ==']'):
            if(Mystack.peek()=='['):
                return True
            return False           
        if(item =='}'):
            if(Mystack.peek()=='{'):
                return True
            return False
                
    def tester(String=string):
        for element in String:
            if(element =='(' or element =='{' or element =='['):
                Mystack.push(element)
                if(element ==')' or element =='}' or element ==']'):
                    if(Checker.equal_check(element)==True):
                        Mystack.pop()
                    if(Checker.equal_check(element)==False):
                        falid = 0
Checker()
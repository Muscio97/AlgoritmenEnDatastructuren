from inspect import stack

class Mystack():
    global stack
    stack = []
    
    def push(item):
        stack.extend([item])
        
    def pop():
        if(len(stack) > 0):
            temp = stack[-1]
            del stack[-1]
            return temp
        return 0
    
    def peek():
        return stack[-1]   
    
    def isEmpty 
        del stack[0:-1]
        
Mystack.push(1)   
print(Mystack.peek())
Mystack.push(2)
print(Mystack.peek())
Mystack.push(3)
print(Mystack.peek())
Mystack.pop()
print(Mystack.peek())
Mystack